    <interface>\Device\NPF_Loopback</interface>
    <udpPeriod>1000</udpPeriod>
    <udpPort>1250</udpPort>

    <!--    element information         -->
    <maxP>150</maxP>
<!--    <minP>0</minP>-->
<!--    <currentP>100</currentP>-->
    <maxQ>100</maxQ>
<!--    <currentQ>50</currentQ>-->
